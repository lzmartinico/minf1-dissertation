@article{Sweeney2001,
abstract = {Today's globally networked society places great demand on the dissemination and sharing of$\backslash$nperson-specific data for many new and exciting uses. Even situations where aggregate statistical$\backslash$ninformation was once the reporting norm now rely heavily on the transfer of microscopically detailed$\backslash$ntransaction and encounter information. This happens at a time when more and more historically public$\backslash$ninformation is also electronically available. When these data are linked together, they provide an$\backslash$nelectronic shadow of a person or organization that is as identifying and personal as a fingerprint even$\backslash$nwhen the information contains no explicit identifiers, such as name and phone number. Other distinctive$\backslash$ndata, such as birth date and ZIP code, often combine uniquely and can be linked to publicly available$\backslash$ninformation to re-identify individuals. Producing anonymous data that remains specific enough to be$\backslash$nuseful is often a very difficult task and practice today tends to either incorrectly believe confidentiality is$\backslash$nmaintained when it is not or produces data that are practically useless.$\backslash$nThe goal of the work presented in this book is to explore computational techniques for releasing$\backslash$nuseful information in such a way that the identity of any individual or entity contained in data cannot be$\backslash$nrecognized while the data remain practically useful. I begin by demonstrating ways to learn information$\backslash$nabout entities from publicly available information. I then provide a formal framework for reasoning$\backslash$nabout disclosure control and the ability to infer the identities of entities contained within the data. I$\backslash$nformally define and present null-map, k-map and wrong-map as models of protection. Each model$\backslash$nprovides protection by ensuring that released information maps to no, k or incorrect entities, respectively.$\backslash$nThe book ends by examining four computational systems that attempt to maintain privacy while$\backslash$nreleasing electronic information. These systems are: (1) my Scrub System, which locates personallyidentifying$\backslash$ninformation in letters between doctors and notes written by clinicians; (2) my Datafly II$\backslash$nSystem, which generalizes and suppresses values in field-structured data sets; (3) Statistics Netherlands'$\backslash$nm-Argus System, which is becoming a European standard for producing public-use data; and, (4) my k-$\backslash$nSimilar algorithm, which finds optimal solutions such that data are minimally distorted while still$\backslash$nproviding adequate protection. By introducing anonymity and quality metrics, I show that Datafly II can$\backslash$noverprotect data, Scrub and m-Argus can fail to provide adequate protection, but k-similar finds optimal$\backslash$nresults.},
author = {Sweeney, Latanya},
file = {:Users/Lorenzo/Downloads/Minf/sweeney-thesis-draft.pdf:pdf},
journal = {Computer Science},
pages = {216},
title = {{Computational disclosure control:  A Primer on Data Privacy Protection}},
volume = {Ph.D.},
year = {2001}
