journal = {Memory and Cognition},
keywords = {Categorization,Cross-classification,Induction},
number = {1},
pages = {92--106},
pmid = {21264618},
title = {{Rich in vitamin C or just a convenient snack? Multiple-category reasoning with cross-classified foods}},
volume = {39},
year = {2011}
